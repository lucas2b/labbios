<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>grupoExameBean</managed-bean-name>
		<managed-bean-class>labbios.beans.GrupoExameBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>convenioBean</managed-bean-name>
		<managed-bean-class>labbios.beans.ConvenioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>labbios.beans.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>materialExameBean</managed-bean-name>
		<managed-bean-class>labbios.beans.MaterialExameBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tipoLaboratorioBean</managed-bean-name>
		<managed-bean-class>labbios.beans.TipoLaboratorioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>growlBean</managed-bean-name>
		<managed-bean-class>labbios.beans.growlBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cadastroDeExameBean</managed-bean-name>
		<managed-bean-class>labbios.beans.CadastroDeExameBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>entradaDoSistemaBean</managed-bean-name>
		<managed-bean-class>labbios.beans.entradaDoSistemaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>MenuBean</managed-bean-name>
		<managed-bean-class>labbios.menu.MenuBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>valoresDeExamesBean</managed-bean-name>
		<managed-bean-class>labbios.beans.ValoresDeExamesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<converter>
		<converter-id>ConversorGrupoExame</converter-id>
		<converter-class>labbios.conversores.ConversorGrupoExame</converter-class>
	</converter>
	<converter>
		<converter-id>ConversorMaterialExame</converter-id>
		<converter-class>labbios.conversores.ConversorMaterialExame</converter-class>
	</converter>
	<converter>
		<converter-id>ConversorTipoLaboratorio</converter-id>
		<converter-class>labbios.conversores.ConversorTipoLaboratorio</converter-class>
	</converter>
	<converter>
		<converter-id>ConversorConvenio</converter-id>
		<converter-class>labbios.conversores.ConversorConvenio</converter-class>
	</converter>
	<navigation-rule>
		<display-name>grupoDeExames</display-name>
		<from-view-id>/grupoDeExames.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editarGrupoDeExames</from-outcome>
			<to-view-id>/editarGrupoDeExames.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>editarGrupoDeExames</display-name>
		<from-view-id>/editarGrupoDeExames.jsp</from-view-id>
		<navigation-case>
			<from-outcome>grupoDeExames</from-outcome>
			<to-view-id>/grupoDeExames.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tipoLaboratorio</display-name>
		<from-view-id>/tipoLaboratorio.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editarTipoLaboratorio</from-outcome>
			<to-view-id>/editarTipoDeLaboratorio.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>editarTipoDeLaboratorio</display-name>
		<from-view-id>/editarTipoDeLaboratorio.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listarTiposDeLaboratorio</from-outcome>
			<to-view-id>/tipoLaboratorio.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>materiaisDeExames/materialExame</display-name>
		<from-view-id>/materiaisDeExames/materialExame.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editarMaterialDeExame</from-outcome>
			<to-view-id>/materiaisDeExames/editarMaterialExame.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>materiaisDeExames/editarMaterialExame</display-name>
		<from-view-id>/materiaisDeExames/editarMaterialExame.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listarMateriaisDeExame</from-outcome>
			<to-view-id>/materiaisDeExames/materialExame.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>cadastrarExames.xhtml</display-name>
		<from-view-id>/cadastrarExames.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>associarValorAoExame</from-outcome>
			<to-view-id>/valorExamePorConvenio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>cadastrarExames.xhtml</display-name>
		<from-view-id>/cadastrarExames.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>entrarDadosDoExame</from-outcome>
			<to-view-id>/dadosDeExame.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>validarUsuario</display-name>
		<from-view-id>/validarUsuario.jsp</from-view-id>
		<navigation-case>
			<from-outcome>acessoPermitido</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dadosDeExame.xhtml</display-name>
		<from-view-id>/dadosDeExame.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listarExames</from-outcome>
			<to-view-id>/cadastrarExames.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
