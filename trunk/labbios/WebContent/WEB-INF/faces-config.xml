<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>grupoExameBean</managed-bean-name>
		<managed-bean-class>labbios.beans.GrupoExameBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>convenioBean</managed-bean-name>
		<managed-bean-class>labbios.beans.ConvenioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>labbios.beans.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>materialExameBean</managed-bean-name>
		<managed-bean-class>labbios.beans.MaterialExameBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tipoLaboratorioBean</managed-bean-name>
		<managed-bean-class>labbios.beans.TipoLaboratorioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>growlBean</managed-bean-name>
		<managed-bean-class>labbios.beans.growlBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>cadastroDeExameBean</managed-bean-name>
		<managed-bean-class>labbios.beans.CadastroDeExameBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>tabelaDePrecosBean</managed-bean-name>
		<managed-bean-class>labbios.beans.tabelaDePrecosBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>validarUsuario</display-name>
		<from-view-id>/validarUsuario.jsp</from-view-id>
		<navigation-case>
			<from-outcome>acessoPermitido</from-outcome>
			<to-view-id>/entradaPrincipal.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>grupoDeExames</display-name>
		<from-view-id>/grupoDeExames.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editarGrupoDeExames</from-outcome>
			<to-view-id>/editarGrupoDeExames.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>editarGrupoDeExames</display-name>
		<from-view-id>/editarGrupoDeExames.jsp</from-view-id>
		<navigation-case>
			<from-outcome>grupoDeExames</from-outcome>
			<to-view-id>/grupoDeExames.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tipoLaboratorio</display-name>
		<from-view-id>/tipoLaboratorio.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editarTipoLaboratorio</from-outcome>
			<to-view-id>/editarTipoDeLaboratorio.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>editarTipoDeLaboratorio</display-name>
		<from-view-id>/editarTipoDeLaboratorio.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listarTiposDeLaboratorio</from-outcome>
			<to-view-id>/tipoLaboratorio.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>materiaisDeExames/materialExame</display-name>
		<from-view-id>/materiaisDeExames/materialExame.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editarMaterialDeExame</from-outcome>
			<to-view-id>/materiaisDeExames/editarMaterialExame.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>materiaisDeExames/editarMaterialExame</display-name>
		<from-view-id>/materiaisDeExames/editarMaterialExame.jsp</from-view-id>
		<navigation-case>
			<from-outcome>listarMateriaisDeExame</from-outcome>
			<to-view-id>/materiaisDeExames/materialExame.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>testePrimeFaces.xhtml</display-name>
		<from-view-id>/testePrimeFaces.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>atualizar</from-outcome>
			<to-view-id>/testePrimeFaces.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
